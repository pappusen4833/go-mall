import request from '@/utils/request'

export function add(data) {
  return request({
    url: 'admin/{{.BusinessName}}',
    method: 'post',
    data
  })
}

export function del(ids) {
  return request({
    url: 'admin/{{.BusinessName}}/',
    method: 'delete',
    data: ids
  })
}

export function edit(data) {
  return request({
    url: 'admin/{{.BusinessName}}',
    method: 'put',
    data
  })
}

export default { add, edit, del }
